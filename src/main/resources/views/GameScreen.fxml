<?import javafx.scene.layout.Pane?>

<?import java.net.URL?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<Pane fx:controller="sample.controllers.game.GameController"
      xmlns:fx="http://javafx.com/fxml" fx:id="pane" onKeyPressed="#keyClicked" onKeyReleased="#keyReleased" prefHeight="720.0" prefWidth="1280.0">
    <stylesheets>
        <URL value="@../stylesheets/GameScreen.css"/>
    </stylesheets>
    <children>
        <HBox>
            <children>
                <Button onAction="#backClicked" styleClass="button" text="BACK">
                    <HBox.margin>
                        <Insets bottom="100.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</Pane>